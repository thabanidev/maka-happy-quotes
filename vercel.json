{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(css|js)/.*",
        "headers": { "cache-control": "s-maxage=31536000,immutable" },
        "continue": true
      },
      {
        "src": "/(.*)",
        "dest": "server.js"
      }
    ]
  }